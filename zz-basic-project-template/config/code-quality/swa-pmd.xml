  <rule class="net.sourceforge.pmd.lang.rule.XPathRule" message="Fields must be private and have accessor methods" name="XPathRule_1336500633" language="java">
    <priority>3</priority>
    <properties>
      <property name="xpath">
        <value><![CDATA[//ClassOrInterfaceBodyDeclaration
  /FieldDeclaration[
    not(@Private='true')
