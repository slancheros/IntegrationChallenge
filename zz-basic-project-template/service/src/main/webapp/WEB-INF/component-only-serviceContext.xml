<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
      http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- A Spring config file that bootstraps and wires in the service. It is 
		called from web.xml -->

	<!-- Use plain CXF wiring by default -->
	<import resource="classpath:zz-appContext.xml" />
	 <!-- import tbs status endpoint configuration -->
	<import resource="classpath:tbsStatusServiceContext.xml" />
  	<!-- import the PCI compliant security specific configuration -->
  	<import resource="classpath:tbsPentaSecuredServiceContext.xml" />
  	
</beans>
