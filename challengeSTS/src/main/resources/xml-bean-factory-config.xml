<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-3.1.xsd 
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<bean id="complete" class="com.swacorp.training.CompleteStrategy">
	</bean>
	<bean id="bunisess" class="com.swacorp.training.BunisessExceptionStrategy">
	</bean>
	<bean id="badRequest" class="com.swacorp.training.BadRequestStrategy">
	</bean>
	<bean id="serverError" class="com.swacorp.training.ServerErrorStrategy">
	</bean>

</beans> 